men_Main MENU
BEGIN
	POPUP "&File"
	BEGIN
		MENUITEM "&New", cm_New
		MENUITEM "&Open...", cm_Open
		MENUITEM SEPARATOR
		MENUITEM "&Save", cm_Save, GRAYED
		MENUITEM "Save &as...", cm_SaveAs, GRAYED
		MENUITEM SEPARATOR
		MENUITEM "&Print...", cm_Print, GRAYED
		MENUITEM SEPARATOR
		MENUITEM "E&xit", cm_Exit
	END

	POPUP "&Edit"
	BEGIN
		MENUITEM "&Undo      Alt+Backspace", cm_EditUndo, GRAYED
		MENUITEM SEPARATOR
		MENUITEM "Cu&t         Shift+Del", cm_EditCut, GRAYED
		MENUITEM "&Copy      Ctrl+Ins", cm_EditCopy, GRAYED
		MENUITEM "&Paste     Shift+Ins", cm_EditPaste, GRAYED
		MENUITEM SEPARATOR
		MENUITEM "Cl&ear     ", cm_EditClear, GRAYED
		MENUITEM "D&elete", cm_EditDelete, GRAYED
	END

	POPUP "&Options"
	BEGIN
		MENUITEM "&Directories...", cm_Directories
		MENUITEM SEPARATOR
		POPUP "&Environment"
		BEGIN
			MENUITEM "&Preferences...", cm_Preferences
			MENUITEM "&Mouse...", cm_Mouse
		END

		MENUITEM SEPARATOR
		MENUITEM "&Open...", cm_Options_Open, GRAYED
		MENUITEM "&Save", cm_Options_Save, GRAYED
		MENUITEM "Save &as...", cm_Options_SaveAs, GRAYED
	END

	POPUP "&Window"
	BEGIN
		MENUITEM "&Tile", cm_TileChildren, GRAYED
		MENUITEM "&Cascade", cm_CascadeChildren, GRAYED
		MENUITEM "Arrange &icons", cm_ArrangeIcons, GRAYED
		MENUITEM "Close &all", cm_CloseChildren, GRAYED
	END

	POPUP "&Help"
	BEGIN
		MENUITEM "&Index               Shift+F1", cm_Index, GRAYED
		MENUITEM "&Topic Search    Ctrl+F1", cm_Topic_Search, GRAYED
		MENUITEM "&Glossary", cm_Glossary, GRAYED
		MENUITEM "Using &Help", cm_Using_Help, GRAYED
		MENUITEM SEPARATOR
		MENUITEM "&About RWPDemo...", 145
	END

END

