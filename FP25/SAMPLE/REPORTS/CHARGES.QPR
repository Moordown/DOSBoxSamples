SELECT CREDCARD.CARD_ID, CREDCARD.STARTBAL, CREDCARD.BALDATE,;
  DETAILS.TRANS_TYPE, DETAILS.TRANS_DATE, DETAILS.AMT, CREDCARD.TYPE;
 FROM CREDCARD, DETAILS;
 WHERE DETAILS.CARD_ID = CREDCARD.CARD_ID;
   AND DETAILS.TRANS_DATE > CREDCARD.BALDATE;
   AND CREDCARD.CARD_ID <> "    ";
 ORDER BY CREDCARD.CARD_ID;
 INTO CURSOR CHARGES
REPORT FORM REPORTS\CHARGES.FRX PREVIEW
