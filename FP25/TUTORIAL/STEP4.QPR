SELECT OFFICES.CITY, SALESMAN.ONO, INVOICES.SALESMAN, SALESMAN.NAME,;
  CUSTOMER.COMPANY, SUM(INVOICES.ITOTAL);
 FROM INVOICES, SALESMAN, OFFICES, CUSTOMER;
 WHERE SALESMAN.SALESMAN = INVOICES.SALESMAN;
   AND OFFICES.ONO = SALESMAN.ONO;
   AND CUSTOMER.CNO = INVOICES.CNO;
 GROUP BY SALESMAN.ONO, INVOICES.SALESMAN, INVOICES.CNO;
 INTO CURSOR STEP4
