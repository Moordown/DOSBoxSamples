SELECT OFFICES.CITY, SALESMAN.ONO, INVOICES.SALESMAN, SALESMAN.NAME,;
  SUM(INVOICES.ITOTAL);
 FROM INVOICES, SALESMAN, OFFICES;
 WHERE SALESMAN.SALESMAN = INVOICES.SALESMAN;
   AND OFFICES.ONO = SALESMAN.ONO;
 GROUP BY SALESMAN.ONO, INVOICES.SALESMAN;
